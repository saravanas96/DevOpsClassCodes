pipeline{
      agent any


      tools{
            maven 'mymaven'
          }

      stages{
         stage('Clonerepo') {
             steps{
                  git 'https://github.com/saravanas96/DevOpsClassCodes
                  }
                     }

         stage('Compile'){
                      steps{
                          sh 'mvn compile'
                           }

                        }

          stage('UnitTesting'){
                      steps{
                         sh 'mvn test'
                           }
                              }
           stage('CodeCoverage'){
                       steps{
                           sh 'mvn verify'
                            }
                                }

            stage('Package'){
                       steps{
                           sh 'mvn package'
                            }
                            }

}

}
